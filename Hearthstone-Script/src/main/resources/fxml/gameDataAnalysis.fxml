<?xml version="1.0" encoding="UTF-8"?>

<?import club.xiaojiawei.controls.Switch?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<StackPane xmlns="http://javafx.com/javafx"
           xmlns:fx="http://javafx.com/fxml"
           prefWidth="1000" prefHeight="800"
           fx:id="rootPane"
           style="-fx-padding: 10"
           fx:controller="club.xiaojiawei.hsscript.controller.javafx.GameDataAnalysisController">
    <VBox spacing="20">
        <Label text="分析" contentDisplay="RIGHT">
            <graphic>
                <Switch fx:id="analysisSwitch" status="true"/>
            </graphic>
        </Label>
        <StackPane fx:id="canvasPane" VBox.vgrow="ALWAYS">
            <Canvas fx:id="canvas"/>
        </StackPane>
    </VBox>
</StackPane>
